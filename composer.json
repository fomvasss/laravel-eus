{
    "name": "fomvasss/laravel-eus",
    "description": "A package for generate eloquent unique string value",
    "type": "composer-package",
    "keywords": ["laravel", "unique", "slug"],
    "authors": [
        {
            "name": "Vasyl Fomin",
            "email": "fomvasss@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php" : ">=7.0.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0",
        "illuminate/database": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\LaravelEUS\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\LaravelEUS\\ServiceProvider"
            ],
            "aliases": {
                "EUS": "Fomvasss\\LaravelEUS\\Facades\\EUS"
            }
        }
    }
}
